{
  "compilerOptions": {
    "target": "es2017",
    "baseUrl": ".",
    "moduleResolution": "node",
    "sourceMap": true,
    "paths": {
      "+/*": ["./src/styles/*"],
      "-/*": ["./src/static/*"],
      "~/*": ["./src/*"],
      ">/*": ["./*"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
